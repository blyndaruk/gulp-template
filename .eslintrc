{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "plugins": [
    "dollar-sign"
  ],
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "dollar-sign/dollar-sign": [2, "ignoreProperties"],
    "no-plusplus": ["warn", { "allowForLoopAfterthoughts": true }],
    "import/no-extraneous-dependencies": false,
    "no-unused-vars": "warn",
    "no-param-reassign": "warn"
  }
}
